<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Airways</display-name>
  <servlet>
    <servlet-name>one</servlet-name>
    <servlet-class>air_package.LoginClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>one</servlet-name>
    <url-pattern>/action1</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>two</servlet-name>
    <servlet-class>air_package.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>two</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>LoginPage.html</welcome-file>
    <welcome-file>SignUp.html</welcome-file>
  </welcome-file-list>
</web-app>